<!DOCTYPE html>
<html>

<head>
    <% include partials/head %>
        <link rel="stylesheet" href="css/workshops.css">
        <script>
            var urls = <%- JSON.stringify(workshopUrl) %> ;

        </script>
</head>

<body>
    <% include partials/loader %>
        <div id="home">
            <canvas id="canvas"></canvas>

            <% include partials/header %>
                <div class="uk-position-center main-content ">

                    <div class="scroll-content" id="style-7">
                        <div class="inner-content">
                            <div id="main-list">
                                <div>
                                    <div uk-grid>
                                        <h1 class="heading-text  uk-align-center ">
                                            <span>Workshops</span>
                                        </h1>
                                    </div>
                                    <div class="my-rotate">
                                        <div class="uk-grid-medium" uk-grid>

                                            <div class="boxing-left boxing">
                                                <div class="hexagon iot-image more-details">
                                                    <div class="hexTop"></div>
                                                    <div class="hexBottom"></div>
                                                    <div class="hexagon-hidden hover">
                                                        <div class="hexTop" id="hiddenArea"></div>
                                                        <div class="hexBottom" id="hiddenArea"></div>

                                                        <div class="statement">Internet Of Things</div>
                                                    </div>
                                                </div>
                                            </div>

                                            <div class="boxing-right boxing">
                                                <div class="hexagon scribbled-image more-details">
                                                    <div class="hexTop"></div>
                                                    <div class="hexBottom"></div>
                                                    <div class="hexagon-hidden hover">
                                                        <div class="hexTop" id="hiddenArea"></div>
                                                        <div class="hexBottom" id="hiddenArea"></div>

                                                        <div class="statement">Scribbled Stories</div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>




                                    </div>
                                </div>
                            </div>



                        </div>

                    </div>


                </div>






                <% include partials/leftbar %>



                    <% include partials/rightbar %>

                        <% include partials/footer %>




        </div>

        <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
        <!-- UIkit JS -->
        <script src="js/uikit.min.js"></script>
        <script src="js/uikit-icons.min.js"></script>
        <script src="js/canvasloader.js"></script>
        <script src="js/slowStar.js"></script>

        <script>

            $(".more-details").click(function () {
                next = $(this).find('div.statement').get('0').innerHTML;

                console.log(urls);
                urls.forEach(element => {

                    if (element.eventName == next) {

                        window.location = window.location + element.eventUrl;

                    }
                });

            });

        </script>

</body>

</html>